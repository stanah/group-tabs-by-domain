{
  "manifest_version": 2,
  "name": "TST Group Tabs by Domain",
  "version": "0.0.4",
  "description": "Groups Tree Style Tabs by Domain",
  "browser_specific_settings": {
    "gecko": {
      "id": "tst-group-tabs-by-domain@stanah"
    }
  },
  "browser_action": {},
  "icons": {
    "16": "icons/icon.svg",
    "48": "icons/icon.svg",
    "96": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "background": {
    "page": "bg.html"
  },
  "permissions": ["tabs", "contextMenus", "<all_urls>", "storage"]
}
